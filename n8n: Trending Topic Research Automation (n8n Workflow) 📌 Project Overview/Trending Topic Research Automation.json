    {
      "parameters": {
        "jsCode": "// 1. Extract raw JSON text from OpenAI or fallback\nconst raw = $json.output?.[0]?.content?.[0]?.text || \"{}\";\n\n// 2. Clean formatting (remove backticks or accidental markdown)\nconst cleaned = raw\n  .replace(/```json/gi, \"\")\n  .replace(/```/g, \"\")\n  .replace(/json\\n/gi, \"\")\n  .trim();\n\n// 3. Parse JSON safely\nlet parsed;\ntry {\n  parsed = JSON.parse(cleaned);\n} catch (e) {\n  parsed = { tweets: [] };\n}\n\n// 4. Extract tweets\nconst tweets = parsed.tweets || [];\n\n// 5. Get keyword\nconst keyword = $node[\"Pick Random Keyword\"].json.keyword || \"\";\n\n// 6. Normalize tweet objects\nreturn tweets.map((t, i) => ({\n  json: {\n    id: \"tweet_\" + (i + 1),\n    text: t.text,\n    author: t.author,\n    favorites: Math.floor(Math.random() * 300),\n    retweets: Math.floor(Math.random() * 100),\n    timestamp: t.timestamp,\n    url: \"https://twitter.com/x/status/\" + (900000 + i),\n    source: \"twitter\",\n    keyword\n  }\n}));\n"
      },
