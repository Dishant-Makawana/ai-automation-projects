      "id": "c50e0f1b-dfc6-42c3-bfb6-7c16df4c2318",
      "name": "Normalize YouTube"
    },
    {
      "parameters": {
        "jsCode": "const items = $input.all();\nconst normalized = [];\n\nfor (const item of items) {\n  const posts = item.json.data?.children || [];\n\n  for (const p of posts) {\n    const d = p.data;\n\n    normalized.push({\n      source: \"reddit\",\n      title: d.title || \"\",\n      text: d.selftext || \"\",\n      url: \"https://www.reddit.com\" + (d.permalink || \"\"),\n      author: d.author || \"\",\n      published_at: d.created_utc\n        ? new Date(d.created_utc * 1000).toISOString()\n        : \"\",\n      metrics: {\n        upvotes: d.ups || 0,\n        comments: d.num_comments || 0\n      }\n    });\n  }\n}\n\nreturn normalized.map(n => ({ json: n }));\n"
      },
