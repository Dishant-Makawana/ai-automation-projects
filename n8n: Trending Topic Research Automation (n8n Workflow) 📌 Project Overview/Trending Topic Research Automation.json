      "name": "Normalize Reddit"
    },
    {
      "parameters": {
        "jsCode": "const items = $input.all();\nconst normalized = [];\n\nfor (const item of items) {\n  const articles = item.json.articles || [];\n\n  for (const a of articles) {\n    normalized.push({\n      source: \"news\",\n      title: a.title || \"\",\n      text: a.description || \"\",\n      url: a.url || \"\",\n      author: a.author || \"\",\n      published_at: a.publishedAt || \"\",\n      metrics: {\n        relevance: null\n      },\n      source_name: a.source?.name || \"\"\n    });\n  }\n}\n\nreturn normalized.map(n => ({ json: n }));\n"
      },
